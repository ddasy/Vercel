{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/index.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/health",
      "dest": "/api/index.js",
      "methods": ["GET"]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["hnd1"]
} 